[Desktop Entry]
Type=Application
X-Nemo-Application-Type=generic
Name=@info_name@
Icon=@info_app_icon_id@
NoDisplay=true
MimeType=image/*;audio/*;video/*;x-scheme-handler/http;x-scheme-handler/https;
X-Maemo-Service=@info_dbus_app_service@
X-Maemo-Method=@info_dbus_app_service@.Playlist.openUrl

[X-Sailjail]
Sandboxing=Disabled
Permissions=Internet;Audio;Microphone;RemovableMedia;UserDirs;MediaIndexing;JupiiGpodder
OrganizationName=@info_org@
ApplicationName=@info_id@
ExecDBus=@info_binary_id@
